{
  "manifest_version": 2,
  "name": "Cache It",
  "version": "1.0",
  "description": "Injects 'cache:' in front of URLs and provides cache options.",
  "permissions": ["activeTab", "tabs"],
  "browser_action": {
    
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
